{
  "name": "technicianmarketplace",
  "main": "./App.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:components": "jest --testPathPattern=__tests__/components",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:firebase": "cp firestore.rules firestore.rules.prod && cp firestore.rules.test firestore.rules && firebase emulators:exec 'npm run test -- src/__tests__/integration/firebase-emulator.test.js' && cp firestore.rules.prod firestore.rules",
    "test:emulator": "cp firestore.rules firestore.rules.prod && cp firestore.rules.test firestore.rules && npm test -- src/__tests__/integration/firebase-emulator.test.js && cp firestore.rules.prod firestore.rules",
    "test:firebase:watch": "jest --testPathPattern=firebase-emulator --watch",
    "test:all": "cp firestore.rules firestore.rules.prod && cp firestore.rules.test firestore.rules && firebase emulators:exec 'npm run test' && cp firestore.rules.prod firestore.rules",
    "test:all:coverage": "cp firestore.rules firestore.rules.prod && cp firestore.rules.test firestore.rules && firebase emulators:exec 'npm run test -- --coverage' && cp firestore.rules.prod firestore.rules",
    "emulator:start": "firebase emulators:start",
    "emulator:import": "firebase emulators:export ./emulator-data",
    "emulator:export": "firebase emulators:import ./emulator-data",
    "e2e": "detox test -c ios.sim.debug -l verbose",
    "e2e:build": "detox build-framework-cache && detox build-app -c ios.sim.debug"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/stack": "^7.6.13",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.2",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~15.0.3",
    "expo-linking": "~8.0.11",
    "expo-router": "^6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-razorpay": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.28.6",
    "@babel/plugin-transform-object-rest-spread": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@firebase/testing": "^0.20.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-native": "^13.3.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "~19.1.0",
    "detox": "^20.46.3",
    "detox-cli": "^20.46.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-fetch": "^2.7.0",
    "react-test-renderer": "^19.1.0",
    "redux-mock-store": "^1.5.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
